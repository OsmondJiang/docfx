{
	"name": "docfx-preview",
	"description": "Docfx preview feature",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:docfx.watch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "docfx.watch",
				"title": "Docfx Preview",
				"category": "Docfx"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.32.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.7"
	}
}
